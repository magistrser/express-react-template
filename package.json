{
    "name": "express-react-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "webpack:client:config": "npx tsc --module commonjs --esmoduleinterop true --skiplibcheck true  webpack.client.config.ts",
        "build:client": "npm run webpack:client:config && webpack --mode production --config webpack.client.config.js",
        "build:server": "webpack --mode production --config webpack.server.config.ts",
        "dev:client": "npm run webpack:client:config && webpack serve --mode development --open --config webpack.client.config.js",
        "dev:server": "nodemon",
        "clean": "prettier src --write --no-error-on-unmatched-pattern",
        "check:ts": "tsc --noEmit -p tsconfig.client.json && tsc --noEmit -p tsconfig.server.json",
        "check:eslint": "eslint ."
    },
    "dependencies": {
        "@mui/icons-material": "^5.15.20",
        "@mui/material": "^5.15.20",
        "@reduxjs/toolkit": "^2.2.5",
        "assert-never": "^1.2.1",
        "axios": "^1.7.2",
        "base-64": "^1.0.0",
        "buffer": "^6.0.3",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "express-promise-router": "^4.1.1",
        "express-winston": "^4.2.0",
        "joi": "^17.13.1",
        "lodash": "^4.17.21",
        "mobx": "^6.12.3",
        "mobx-react-lite": "^4.0.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-redux": "^9.1.2",
        "react-router": "^6.23.1",
        "react-router-dom": "^6.23.1",
        "react-query": "^3.39.3",
        "styled-components": "^6.1.11",
        "typescript": "^5.4.5",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@types/base-64": "^1.0.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.5",
        "@types/node": "^20.14.2",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/styled-components": "^5.1.34",
        "@types/webpack": "^5.28.5",
        "@typescript-eslint/eslint-plugin": "^7.13.0",
        "@typescript-eslint/parser": "^7.13.0",
        "assert-never": "^1.2.1",
        "eslint": "^8.56.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-webpack-plugin": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "html-webpack-plugin": "^5.6.0",
        "nodemon": "^3.1.3",
        "prettier": "^3.3.2",
        "prettier-plugin-organize-imports": "^3.2.4",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "webpack": "^5.92.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
    }
}
